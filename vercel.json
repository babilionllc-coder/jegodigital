{
  "rewrites": [
    {
      "source": "/agents",
      "destination": "/jegodigital-agents-dashboard.html"
    },
    {
      "source": "/webhook",
      "destination": "/api/whatsapp"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
